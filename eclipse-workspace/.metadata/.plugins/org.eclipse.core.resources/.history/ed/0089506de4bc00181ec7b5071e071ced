package compositor;

enum Figures {
	SEMIBREVE("Semibreve"),
	MINIM("Blanca"),
	CROTCHET("Negra"),
	QUAVER("Corchea"),
	SEMIQUAVER("Semicorchea"),
	DEMISEMIQUAVER("Fusa"),
	HEMIDEMISEMIQUAVER("Semicorchea");

	Figures(String symbol){}
	
	public String toString() {
		return name().toLowerCase();
	}
}